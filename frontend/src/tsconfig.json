{
    "compilerOptions": {
      "target": "es5",
      "module": "es2015",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "sourceMap": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["*"]
      },
      "allowJs": true,
      "jsx": "preserve",
      "experimentalDecorators": true
    },
    "exclude": ["node_modules"]
  }